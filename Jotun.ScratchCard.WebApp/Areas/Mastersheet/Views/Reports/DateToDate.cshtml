@{
    ViewData["Title"] = "Daily Report";
}

<div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
    <div class="breadcrumb-title pe-3">Report</div>
    <div class="ps-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item active">
                    <a href="javascript:;"><i class="bx bx-home-alt"></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">Date to Date Report</li>
            </ol>
        </nav>
    </div>
</div>

<div class="row">
    <div class="col-12">
        <div class="card radius-10">
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="input-group mb-3">
                            <label for="txtClnFromDate" class="form-label w-100 d-block">From Date</label>
                            <input type="text" class="form-control p_kendo_date" id="txtClnFromDate">
                        </div>
                    </div>

                    <div class="col-lg-4">
                        <div class="input-group mb-3">
                            <label for="txtClnToDate" class="form-label w-100 d-block">To Date</label>
                            <input type="text" class="form-control p_kendo_date" id="txtClnToDate">
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <button type="button" class="btn btn-dark px-5"><i class="bx bxs-cloud-download mr-1"></i>Downloads</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>